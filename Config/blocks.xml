<configs>
	<!-- Ember piles -->
	<!-- All the ember piles extends from emberPileDeco1 -->
	<append xpath="/blocks/block[@name='emberPileDeco1']">
		<property name="EconomicValue" value="20"/>
	</append>
	<!-- Camping chairs -->
	<!-- All the camping chairs extends from chairCampingWhite -->
	<append xpath="/blocks/block[@name='chairCampingWhite']">
		<property name="EconomicValue" value="80"/>
	</append>
	<!-- Park benches-->
	<!-- All the park benches extends from parkBenchA. By default are bundled in 20 units -->
	<append xpath="/blocks/block[@name='parkBenchA']">
		<property name="EconomicValue" value="80"/>
		<property name="EconomicBundleSize" value="1"/>
	</append>
	<!-- Playground equipment -->
	<append xpath="/blocks/block[@name='decoMetalSlide']">
		<property name="EconomicValue" value="120"/>
	</append>
	<append xpath="/blocks/block[@name='decoSwingSet']">
		<property name="EconomicValue" value="120"/>
	</append>
	<append xpath="/blocks/block[@name='decoBasketBallHoop']">
		<property name="EconomicValue" value="100"/>
	</append>
	<!-- Hanging plants -->
	<append xpath="/blocks/block[@name='hangingMoss']">
		<property name="EconomicValue" value="1"/>
		<property name="SellableToTrader" value="false"/>
	</append>
	<append xpath="/blocks/block[@name='cobweb']">
		<property name="EconomicValue" value="1"/>
		<property name="SellableToTrader" value="false"/>
	</append>
	<append xpath="/blocks/block[@name='plantHangingIvy01']">
		<property name="EconomicValue" value="1"/>
		<property name="SellableToTrader" value="false"/>
	</append>
	<append xpath="/blocks/block[@name='plantHangingIvy02']">
		<property name="EconomicValue" value="1"/>
		<property name="SellableToTrader" value="false"/>
	</append>
	<!-- Gravestones -->
	<!-- All the gravestones extends from graveStone01 -->
	<append xpath="/blocks/block[@name='graveStone01']">
		<property name="EconomicValue" value="1"/>
		<property name="SellableToTrader" value="false"/>
	</append>		
</configs>